<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

  <bean name="emp" class="com.wipro.hrms.model.Employee" scope="prototype">
  	<property name="id" value="123" />
  	<property name="firstName" value="Raja" />
  	<property name="lastName" value="Sivakumar" />
  	<property name="salary" value="123456.5" />
  	<property name="department" ref="dept1" />
  </bean>

  <bean name="emp2" class="com.wipro.hrms.model.Employee" scope="prototype">
  	<property name="id" value="124" />
  	<property name="firstName" value="Kumar" />
  	<property name="lastName" value="Sonu" />
  	<property name="salary" value="12343.5" />
  	<property name="department" ref="dept1" />
  </bean>

	<bean name="dept" class="com.wipro.hrms.model.Department">
		<constructor-arg index="0" type="java.lang.Integer" value="1" />
		<constructor-arg index="1" type="java.lang.String" value="ECE" />
	</bean>
	
		<bean name="dept1" class="com.wipro.hrms.model.Department">
		<property name="id" value="13" />
		<property name="name" value="CSE" />
	</bean>
</beans>