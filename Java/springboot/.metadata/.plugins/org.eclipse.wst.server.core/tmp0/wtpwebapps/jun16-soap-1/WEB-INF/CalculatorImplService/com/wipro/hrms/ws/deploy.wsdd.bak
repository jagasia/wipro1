<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from CalculatorImplService WSDL service -->

  <service name="CalculatorImpl" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://ws.hrms.wipro.com"/>
      <parameter name="wsdlServiceElement" value="CalculatorImplService"/>
      <parameter name="schemaQualified" value="http://ws.hrms.wipro.com"/>
      <parameter name="wsdlServicePort" value="CalculatorImpl"/>
      <parameter name="className" value="com.wipro.hrms.ws.CalculatorImplSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="CalculatorImpl"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="sum" qname="operNS:sum" xmlns:operNS="http://ws.hrms.wipro.com" returnQName="retNS:sumReturn" xmlns:retNS="http://ws.hrms.wipro.com" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:i" xmlns:pns="http://ws.hrms.wipro.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:j" xmlns:pns="http://ws.hrms.wipro.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="difference" qname="operNS:difference" xmlns:operNS="http://ws.hrms.wipro.com" returnQName="retNS:differenceReturn" xmlns:retNS="http://ws.hrms.wipro.com" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:i" xmlns:pns="http://ws.hrms.wipro.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:j" xmlns:pns="http://ws.hrms.wipro.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="product" qname="operNS:product" xmlns:operNS="http://ws.hrms.wipro.com" returnQName="retNS:productReturn" xmlns:retNS="http://ws.hrms.wipro.com" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:i" xmlns:pns="http://ws.hrms.wipro.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:j" xmlns:pns="http://ws.hrms.wipro.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="product difference sum"/>

  </service>
</deployment>
